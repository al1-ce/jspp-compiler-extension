{
    "version": "2.0.0",
    "tasks": [{
        "label": "build",
        "type": "shell",
        "command": "/e/jspp/jsppext.exe src/ -o js/",
        "group": {
            "kind": "build",
            "isDefault": true
        },
        "problemMatcher": [{
            "owner": "js++",
            "source": "js++",
            "pattern": [{
                    "file": 1, "line": 2, "column": 3, "code": 4, "message": 5, 
                    "regexp": "(.*?)\\((\\d+)\\,(\\d+)\\)\\: Error\\[(.*?)\\]\\: (.*)"
            }]
        }]
    }]
}